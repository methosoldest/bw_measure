{
  "name": "bw-measurement",
  "description": "Testing Bandwidth Measurement",
  "version": "2.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
  {
      "matches": ["http://*/*", "https://*/*"],
      "css" : ["bw.css"],
      "js" : ["bw.js"]
  }
  ], 
  "permissions" : [
    "tabs",	
    "webRequest",
    "webRequestBlocking",
    "*://*/"
	 ],			
  "manifest_version": 2
}
